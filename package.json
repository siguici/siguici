{
  "name": "sigui.ci",
  "description": "Sigui's web application.",
  "engines": {
    "bun": ">=1.0.4"
  },
  "packageManager": "bun@1.0.4",
  "publishConfig": {
    "access": "public"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "deno task check && astro check",
    "deploy": "deployctl deploy --project=sigui --no-static --include=./dist ./dist/server/entry.mjs",
    "dev": "astro dev",
    "preview": "bun run build && bun run serve",
    "prod": "bun run build && bun run deploy",
    "serve": "deno run --allow-net --allow-read --allow-env dist/server/entry.mjs",
    "start": "astro dev --open"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.0",
    "@astrojs/deno": "^5.0.1",
    "@astrojs/svelte": "^4.0.2",
    "@astrojs/tailwind": "^5.0.0",
    "astro": "^3.2.0",
    "svelte": "^4.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.7.2",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nested": "^6.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2"
  }
}
